{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    {
      "src": "/dashboard",
      "dest": "templates/dashboard.html"
    }
    ]
}
